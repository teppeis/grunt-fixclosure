  },
  "peerDependencies": {
    "grunt": ">=0.4.0"
  },
  "homepage": "https://github.com/teppeis/grunt-fixclosure",
  "repository": {
    "type": "git",
    "url": "https://github.com/teppeis/grunt-fixclosure"
  },
  "bugs": "https://github.com/teppeis/grunt-fixclosure/issues",
